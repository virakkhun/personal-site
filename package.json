{
  "name": "virak.me",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "web:dev": "yarn --cwd web dev",
    "web:build": "yarn --cwd web build",
    "web:preview": "yarn --cwd web preview",
    "web:check": "yarn --cwd web svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "web:check:watch": "yarn --cwd web svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "web:test": "yarn --cwd web vitest",
    "web:lint": "yarn --cwd web prettier --plugin-search-dir . --check . && eslint .",
    "web:format": "yarn --cwd web prettier --plugin-search-dir . --write .",
    "dashboard:dev": "yarn --cwd dashboard dev",
    "dashboard:build": "yarn --cwd dashboard build",
    "dashboard:preview": "yarn --cwd dashboard preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.22.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.26",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.34.3"
  },
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.31.0",
    "@ui/virak.me": "1.0.0",
    "refractor": "^4.8.1",
    "rehype-format": "^4.0.1",
    "rehype-prism-plus": "^1.6.2",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "solid-js": "^1.7.8",
    "unified": "^10.1.2"
  },
  "workspaces": [
    "ui",
    "dashboard",
    "web"
  ]
}
